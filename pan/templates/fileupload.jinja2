{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
    <h1>File Upload</h1>
    <span class="font-semi-bold">{{message}}</span>

    <form action="{{url}}" method="post" enctype="multipart/form-data">
        <label for="login">文件</label>
        <input type="file" id="file"
               name="file" /><br/>
        <input type="submit" name="form.upload"
               value="Upload"/>
    </form>

</div>


{% endblock content %}